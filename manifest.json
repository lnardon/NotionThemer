{
  "manifest_version": 2,
  "name": "NotionThemer",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Notion Themer"
  }
}
